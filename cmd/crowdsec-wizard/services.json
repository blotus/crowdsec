{
    "apache2" : {
        "logs_file": [
        "/var/log/apache2/*.log",
        "/var/log/*httpd*.log"
        ],
        "collections" : [
        "crowdsecurity/nginx",
        "crowdsecurity/base-http-scenarios"
        ]
    },
    "nginx" : {
        "logs_file": [
        "/var/log/nginx/*.log"
        ],
        "collections" : [
            "crowdsecurity/nginx",
            "crowdsecurity/base-http-scenarios"
        ]
    },
    "syslog" : {
        "logs_file": [
            "/var/log/syslog",
            "/var/log/kern.log",
            "/var/log/messages"
        ],
        "collections" : [
            "crowdsecurity/linux"
        ]
    }
}